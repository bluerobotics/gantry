{
    "main_state": {
        "type": "string",
        "default": "standby",
        "help": "Overall gantry system state"
    },
    "x_state": {
        "type": "string",
        "default": "standby",
        "help": "Current operational state of X-axis"
    },
    "y_state": {
        "type": "string",
        "default": "standby",
        "help": "Current operational state of Y-axis"
    },
    "z_state": {
        "type": "string",
        "default": "standby",
        "help": "Current operational state of Z-axis"
    },
    "x_pos": {
        "unit": "mm",
        "type": "float",
        "default": 0.0,
        "precision": 2,
        "help": "Current position of X-axis"
    },
    "x_torque": {
        "unit": "%",
        "type": "float",
        "default": 0.0,
        "precision": 1,
        "help": "Current motor torque percentage for X-axis"
    },
    "x_enabled": {
        "type": "int",
        "default": 1,
        "map": {
            "0": "disabled",
            "1": "enabled"
        },
        "help": "Power enable status for X-axis motor"
    },
    "x_homed": {
        "type": "int",
        "default": 0,
        "map": {
            "0": "not homed",
            "1": "homed"
        },
        "help": "Indicates if X-axis has been homed"
    },
    "y_pos": {
        "unit": "mm",
        "type": "float",
        "default": 0.0,
        "precision": 2,
        "help": "Current position of Y-axis"
    },
    "y_torque": {
        "unit": "%",
        "type": "float",
        "default": 0.0,
        "precision": 1,
        "help": "Current motor torque percentage for Y-axis"
    },
    "y_enabled": {
        "type": "int",
        "default": 1,
        "map": {
            "0": "disabled",
            "1": "enabled"
        },
        "help": "Power enable status for Y-axis motor"
    },
    "y_homed": {
        "type": "int",
        "default": 0,
        "map": {
            "0": "not homed",
            "1": "homed"
        },
        "help": "Indicates if Y-axis has been homed"
    },
    "z_pos": {
        "unit": "mm",
        "type": "float",
        "default": 0.0,
        "precision": 2,
        "help": "Current position of Z-axis"
    },
    "z_torque": {
        "unit": "%",
        "type": "float",
